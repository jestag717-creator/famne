local use_bypass = true
local auto_tutorial = false
local auto_malady = false
local auto_rotation = false
local pack_name = "world_lock"
local pack_price = 2000
local platformID = 0

local text_spam_grumbleteeth = {
    "Aku akan musnahkan seluruh alien yang datang ke bumi",
    "Aku adalah Super Hero",
    "Akan ku hancurkan Pepper Block ini dalam 1 hit",
    "Gass",
    "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
}

local bots = {}

local function setupBotCommon(bot)
    bot:getConsole().enabled = true
    bot.bypass_logon = use_bypass
    bot.auto_ban = true
    
    local tutorial = bot.auto_tutorial
    tutorial.enabled = auto_tutorial
    tutorial.auto_quest = true
    tutorial.set_as_home = true
    tutorial.set_high_level = true
    tutorial.set_random_skin = false
    tutorial.set_random_profile = false
    
    local spam = bot.auto_spam
    spam.auto_interval = true
    spam.show_emote = false
    spam.randomizer = false
    spam.use_color = true
    spam.messages = text_spam_grumbleteeth
    spam.enabled = false
    
    local malady = bot.auto_malady
    malady.auto_grumbleteeth = true
    malady.auto_chicken_feet = true
    malady.use_home = true
    malady.auto_disconnect = false
    malady.enabled = auto_malady
    
    local rotation = bot.rotation
    rotation.harvest_until_level = true
    rotation.tiles = 135296
    bot.gem_limit = pack_price
    bot:setPack(pack_name)
    rotation.enabled = auto_rotation
    
    table.insert(bots, bot)
end

local function processAccount(account)
    account = account:match("^%s*(.-)%s*$")
    
    if account == "" then return end
    
    local bot
    
    local email, sisa = account:match("([^|]+)|(.+)")
    if email and sisa then
        local mac, remaining = sisa:match("([A-Fa-f0-9][A-Fa-f0-9]:[A-Fa-f0-9][A-Fa-f0-9]:[A-Fa-f0-9][A-Fa-f0-9]:[A-Fa-f0-9][A-Fa-f0-9]:[A-Fa-f0-9][A-Fa-f0-9]:[A-Fa-f0-9][A-Fa-f0-9]):(.+)")
        
        if mac and remaining then
            local rid, wk, ltoken = remaining:match("([A-Fa-f0-9]+):([A-Fa-f0-9]+):(.+)")
            
            if rid and wk and ltoken then
                
                local details = {
                    ["name"] = ltoken,
                    ["rid"] = rid,
                    ["mac"] = mac,
                    ["wk"] = wk,
                    ["platform"] = platformID,
                }
                bot = addBot(details)
            else
                print("Failed to parse RID/WK/LTOKEN from: " .. remaining)
                return
            end
        else
            print("Failed to parse MAC address from: " .. sisa)
            return
        end
    else
        if account:match("^[^@]+@[^@]+%.[^@]+$") then
            print("Adding bot with email format: " .. account)
            bot = addBot(account, "xxxx")
        else
            print("Invalid account format: " .. account)
            return
        end
    end
    
    if bot then
        setupBotCommon(bot)
        sleep(3)
    end
end

print("Starting bot setup...")
for account in accounts:gmatch("[^\n]+") do
    processAccount(account)
end

print("Setup complete! Added " .. #bots .. " bots.")
